/*banner 头部*/
*, ::before, ::after {
    padding: 0;
    margin: 0;
    -webkit-tap-highlight-color: transparent;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.banner {
    width: 100%;
}

.banner > div {
    position: relative;
    width: 100%;
}

.banner > div > img {
    display: block;
    width: 100%;
    max-height: 400px;
}

.banner > div > a:nth-child(2) {
    position: absolute;
    top: 0;
    left: 0;
    width: 0.83rem;
    height: 0.82rem;
    background: url("../img/icon_home.png") no-repeat;
    padding: 0.25rem 0.15rem 0.15rem 0.25rem;
    /*宽度和高度*/
    background-size: 0.43rem 0.42rem;
    background-origin: content-box;
}

.banner > div > a:nth-child(3) {
    position: absolute;
    top: 0;
    right: 0;
    width: 0.80rem;
    height: 0.81rem;
    background: url("../img/icon_share.png") no-repeat;
    padding: 0.28rem 0.27rem 0.15rem 0.15rem;
    /*宽度和高度*/
    background-size: 0.38rem 0.38rem;
    background-origin: content-box;
}

.banner > div > a:nth-child(4) {
    position: absolute;
    bottom: 0;
    right: 0;
    width: 1.48rem;
    height: 1.48rem;
    background: url("../img/icon_play.png") no-repeat;
    padding: 0.15rem 0.26rem 0.26rem 0.15rem;
    /*宽度和高度*/
    background-size: 1.07rem 1.07rem;
    background-origin: content-box;
}

.banner > div > span {
    position: absolute;
    left: 0.22rem;
    bottom: 0;
    font-family: "Adobe 黑体 Std";
    font-size: 0.40rem;
    line-height: 0.92rem;
    color: #fff;
}

.box {
    border-radius: 50%;
    border: 1px solid red;
    color: red;
}

/*banner end */
/*单曲列表 */
.singBox {
    width: 100%;
    height: 1.19rem;
    border-bottom: 0.01rem solid #e5e5e5;
    padding: 0.17rem 0 0.14rem 0.2rem;
    overflow: hidden;
}

.singBox > .singTex {
    width: 75%;
    height: 0.88rem;
    overflow: hidden;
    float: left;
}

.singBox > .singTex > .singTexUp {
    width: 100%;
}

.singTex > .singTexUp > .singTexUp2 {
    width: 100%;
    height: 0.5rem;
    font: 0.32rem/0.5rem "微软雅黑";
    color: #333;
    position: relative;
}

/*图标HQ 无损 mv    margin-left  有点问题*/
.singTex > .singTexUp > .singTexUp2 > .spanSingHQ {
    display: inline-block;
    width: 0.42rem;
    height: 0.22rem;
    line-height: 0;
    /*margin-left: 0.08rem;*/
    overflow: hidden;
}

.singTex > .singTexUp > .singTexUp2 > .spanSingHQ > img {
    display: inline-block;
    width: 100%;
}

.singTex > .singTexUp > .singTexUp2 > .spanSingLMusic {
    display: inline-block;
    width: 0.42rem;
    height: 0.22rem;
    line-height: 0;
    overflow: hidden;
}

.singTex > .singTexUp > .singTexUp2 > .spanSingLMusic > img {
    display: inline-block;
    width: 100%;
}

.singTex > .singTexUp > .singTexUp2 > .spanSingMV {
    display: inline-block;
    width: 0.42rem;
    height: 0.22rem;
    line-height: 0;
    overflow: hidden;
}

.singTex > .singTexUp > .singTexUp2 > .spanSingMV > img {
    display: inline-block;
    width: 100%;
}

.singBox > .singTex > .singName {
    width: 100%;
    height: 0.4rem;
    font: 0.22rem/0.4rem "微软雅黑";
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap; /*禁止折行*/
}

.singBox > .singDom2 {
    float: right;
    width: 0.36rem;
    height: 0.36rem;
    box-sizing: content-box;
    padding: 0.26rem 0.28rem 0.26rem 0.26rem;
}

.singBox > .singDom2 > img {
    display: block;
    width: 100%;
}

.singBox > .singMv {
    float: right;
    width: 0.47rem;
    height: 0.36rem;
    box-sizing: content-box;
    padding: 0.26rem 0.26rem 0.26rem 0;
}

.singBox > .singMv > img {
    display: block;
    width: 100%;
}

/*tab 栏*/
.nav {
    width: 100%;
    height: 0.82rem;
    border-bottom: 0.01rem solid #e5e5e5;
}

.nav > .navitem {
    float: left;
    width: 33.3%;
    font-size: 0.32rem;
    text-align: center;
    line-height: 0.80rem;
    color: #333;
    border-bottom: 0.02rem solid transparent;
}

.nav > .navitem.on {
    color: #2095dd;
    border-bottom: 0.02rem solid #2095dd;
}

/*专辑样式*/
.singBox_special {
    width: 100%;
    height: 1.51rem;
    padding: 0.18rem 0 0.12rem 0.2rem;
    display: flex;
    overflow: hidden;
    position: relative;
}

.singBox_special > .specialImgBox {
    width: 1.2rem;
    height: 1.2rem;
    overflow: hidden;
}

.singBox_special > .specialImgBox > img {
    width: 100%;
    display: inline-block;
}

.singBox_special > .Sp_singTex {
    flex: 1;
    margin-left: 0.19rem;
    height: 1.3rem;
    border-bottom: 0.01rem solid #e5e5e5;
}

.singBox_special > .Sp_singTex > .Sp_singTexUp > .singTexUp2 {
    padding-top: 0.14rem;
    font: 0.32rem/0.58rem "Adobe 黑体 Std";
    color: #333;
}

.singBox_special > .Sp_singTex > .Sp_singTexUp > .singTime {
    font: 0.24rem/0.50rem "Adobe 黑体 Std";
    color: #666;
}

.singBox_special > .Sp_singTex > .downLoad {
    position: absolute;
    top: 0;
    right: 0;
    box-sizing: content-box;
    float: left;
    width: 0.36rem;
    height: 0.36rem;
    padding: 0.67rem 0.27rem 0.46rem 0.27rem;
}

.singBox_special > .Sp_singTex > .downLoad > img {
    display: block;
    width: 100%;
}

